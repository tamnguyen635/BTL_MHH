<?xml version="1.0" encoding="UTF-8"?>
<pnml xmlns="http://www.pnml.org/version-2009/std">
  <net id="VendingMachine" type="http://www.pnml.org/version-2009/grammar/ptnet">
    <page id="page1">
      <place id="p_idle"> <initialMarking><text>1</text></initialMarking>
      </place>
      <place id="p_inserted_coin"></place> <place id="p_product_ready"></place> <transition id="t_insert_coin"></transition>
      <arc source="p_idle" target="t_insert_coin"/>
      <arc source="t_insert_coin" target="p_inserted_coin"/>

      <transition id="t_buy"></transition>
      <arc source="p_inserted_coin" target="t_buy"/>
      <arc source="t_buy" target="p_product_ready"/>

      <transition id="t_cancel"></transition>
      <arc source="p_inserted_coin" target="t_cancel"/>
      <arc source="t_cancel" target="p_idle"/> <transition id="t_take_product"></transition>
      <arc source="p_product_ready" target="t_take_product"/>
