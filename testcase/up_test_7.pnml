<pnml xmlns="http://www.pnml.org/version-2009/std">
  <net id="DocProcess" type="http://www.pnml.org/version-2009/grammar/ptnet">
    <page id="page1">
      <place id="p_draft">
        <initialMarking><text>1</text></initialMarking>
      </place>
      <place id="p_review"></place>
      <place id="p_signed"></place> <transition id="t_submit"></transition>
      <arc source="p_draft" target="t_submit"/>
